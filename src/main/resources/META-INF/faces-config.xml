<?xml version='1.0' encoding='UTF-8'?>
<faces-config version="2.0"
              xmlns="http://java.sun.com/xml/ns/javaee" 
              xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
              xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-facesconfig_2_0.xsd">

    <component>
        <component-type>warning</component-type>
        <component-class>com.knitelius.jsfwarn.components.WarningComponent</component-class>
    </component>

    <lifecycle>
        <phase-listener>com.knitelius.jsfwarn.phaselistener.WarningPhaseListener</phase-listener>
    </lifecycle>
</faces-config>
